{
  "extends": "../tsconfig.settings.json",
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist/commonjs",
    "allowJs": true,
    "baseUrl": "src",
    "paths": {
      "*": ["../node_modules/*", "node_modules/*"]
    },
    "jsx": "react"
  },
  "include": ["./*", "__tests__", "src/platforms-config.ts", "src/procedure-router.ts", "src/Twitter", "Twitter/types.ts", "src/index.ts", "src/Brightid/procedures/brightidProcedures.ts"]
}
